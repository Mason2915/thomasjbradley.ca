{
  "name": "thomasjbradley",
  "version": "4.1.1",
  "private": true,
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-concat": "^2.5.2",
    "gulp-cssnano": "^1.0.0",
